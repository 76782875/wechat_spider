#!/usr/bin/env node

var program = require('commander');
var csv = require('../lib/csv.js');

var argValue = '';
program
  .version('1.0.4')
  .arguments('csv')
  .action(function (arg) {
    argValue = arg
  })
  .parse(process.argv);

if (argValue == 'csv') {
  csv.saveAll();
} else {
  var proxy = require('..');
  proxy();
}